@ARTICLE{Fariha:2015,
  author = {Anna Fariha and Chowdhury Farhan Ahmed and Carson K. Leung and Md.
	Samiullah and Suraiya Pervin and Longbing Cao},
  title = {A new framework for mining frequent interaction patterns from meeting
	databases},
  journal = {Engineering Applications of Artificial Intelligence},
  year = {2015},
  volume = {45},
  pages = {103-118},
  owner = {padua},
  review = {Problem: Meetings are very important in workplace. Semantic knowledge
	can be acquired by discovering interaction patterns from these meetings.
	Tree-based approaches may not be the best suitable solution, because
	it cannot model the different relation among members from different
	hierarchical position.
	
	
	Proposed Solution: They proposed the weighted interaction flow directed
	acyclic graph (WIFDAG) to mine the association rules from the meetings.
	First, all the possible interactions are modeled to reduce the domain,
	which means that every interaction taken in the meeting is classified
	as one of the possibles interactions. Then, the weights are distributed
	according to the subject importance, for example, a proposal acceptance
	from the chairman is way more important than an acceptance from a
	junior lecturer. After modeling the interactions, the rules are generated
	using the WIFDAG to find all the frequent interaction rules.
	
	
	Conclusion: The authors showed that this work can be used on real-life
	scenarions, making it better than the others algorithm on literature
	
	
	Notes:},
  timestamp = {2015.11.30}
}

